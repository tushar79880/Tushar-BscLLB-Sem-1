Programe - To find no of days in particular month
Author - Tushar , BscLLb - 1st Semester
Dated on - 25/09/2006
*/
#include<stdio.h>

void main()
{
    int n;
    printf("Enter No. between 1-12: \n");
    scanf(" %d", &n);

    switch(n)
    {
        case 1: printf("Jan - 31");
                break;
                case 2: printf("Feb - 28");
                        break;
                        case 3: printf("Mar - 31");
                                break;
                                case 4: printf("Apr - 30");
                                        break;
                                        case 5: printf("May - 31");
                                                break;
                                                case 6: printf("June - 30");
                                                        break;
                                                        case 7: printf("July - 31");
                                                                break;
                                                                case 8: printf("Aug - 31");
                                                                        break;
                                                                        case 9: printf("Sep - 30");
                                                                                break;
                                                                                case 10: printf("Oct - 31");
                                                                                         break;
                                                                                         case 11: printf("Nov - 30");
                                                                                                  break;
                                                                                                  case 12: printf("Dec - 31");
                                                                                                          break;
                                                                                                          default: printf("Wrong No Entered");
    }


}
