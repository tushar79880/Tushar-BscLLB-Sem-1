/*
Programe: Write first N natural number sum
Author: Tushar , B.Sc.LL.B - 1 Semester
Dated: 17/09/2025
*/
#include <stdio.h>

int main ()
{
    int i, n , sum=0;
    printf("Enter the value of n\t");
    scanf("%d",&n);
    
    for(i=1; i<=n; i++)
    {
        sum=sum+i;
    }
    printf("Sum of 1st n natural number is %d\n", sum);

    return 0;
}
