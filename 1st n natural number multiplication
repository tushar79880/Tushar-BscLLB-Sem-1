/*
Programe: Write first N natural number multiplication
Author: Tushar , B.Sc.LL.B - 1 Semester
Dated: 17/09/2025
*/
#include <stdio.h>

int main ()
{
    int i, n , product=1;
    printf("Enter the value of n\t");
    scanf("%d",&n);
    
    for(i=1; i<=n; i++)
    {
        product=product*i;
    }
    printf("product of 1st natural number is %d\n",product);

    return 0;
}
